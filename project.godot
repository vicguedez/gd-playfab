config_version=5

[application]

config/name="GD PlayFab"
run/main_scene="res://_gd_playfab_test/playfab_test.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://_icon.svg"

[autoload]

PlayFab="*res://addons/gd-playfab/singleton/playfab.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/gd-playfab/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
